{
  "name": "red-site",
  "version": "0.0.1",
  "author": "Grayscale Digital",
  "license": "Private, all rights reserved",
  "private": true,
  "scripts": {
    "clean": "shx rm -f static/styles/*.css && shx rm -f static/js/*.js && shx rm -f data/*.json",
    "predev": "yarn run clean",
    "dev": "webpack --mode development",
    "watch": "webpack --mode development --watch",
    "prebuild": "yarn run clean",
    "build": "webpack --mode production",
    "fmt": "prettier --write \"src/**/*.js\"",
    "lint": "eslint \"src/js/**/*.js\" && stylelint \"src/styles/**/*.css\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run fmt && yarn run lint"
    }
  },
  "devDependencies": {
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^1.0.0-rc.1",
    "mini-css-extract-plugin": "^0.3.0",
    "postcss-loader": "^2.1.3",
    "prettier": "^1.11.1",
    "shx": "^0.2.2",
    "stylelint": "^9.1.3",
    "stylelint-config-standard": "^18.2.0",
    "tailwindcss": "^0.5.1",
    "ts-loader": "^4.1.0",
    "typescript": "^2.8.1",
    "webpack": "^4.3.0",
    "webpack-assets-manifest": "^3.0.1",
    "webpack-cli": "^2.0.13"
  },
  "dependencies": {
    "date-fns": "^1.29.0",
    "vue": "^2.5.16",
    "wretch": "^1.1.2"
  }
}
